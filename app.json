{
  "name": "94Orig",
  "description": "A telegram bot to fetch instagram stories and image",
  "repository": "https://github.com/andrewwang84/94Orig",
  "logo": "https://node-js-sample.herokuapp.com/node.png",
  "keywords": ["node", "telegram", "instagram", "puppeteer", "cheerio"],
  "buildpacks": [
    {
      "url": "https://github.com/jontewks/puppeteer-heroku-buildpack.git"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "NODE_ENV": {
      "description": "Node Env",
      "value": "production"
    },
    "insEmail": {
      "description": "instagram login email for stories",
      "value": ""
    },
    "insPass": {
      "description": "instagram password",
      "value": ""
    },
    "insCookies": {
      "description": "instagram already logged in cookie",
      "value": ""
    },
    "telegramToken": {
      "description": "telegram bot token",
      "value": ""
    },
    "lineAccessToken": {
      "description": "line bot token",
      "value": ""
    },
    "lineSecret": {
      "description": "line bot secret",
      "value": ""
    },
    "twitterToken": {
      "description": "twitter bearer token for api access",
      "value": ""
    }
  }
}
